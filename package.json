  "bugs": {
    "url": "https://github.com/secret-family-recipe-s-cookbook/cookbook-backend-KO/issues"
  },
  "homepage": "https://github.com/secret-family-recipe-s-cookbook/cookbook-backend-KO#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "helmet": "^3.20.0",
    "jsonwebtoken": "^9.0.0",
